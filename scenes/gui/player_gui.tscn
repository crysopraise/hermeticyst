[sub_resource type="Shader" id=42]
code = "shader_type canvas_item;

uniform float threshold : hint_range(0, 1);

void fragment() {
	vec4 alpha = texture(TEXTURE, UV);
	
	if (alpha.r <= threshold && alpha.g <= threshold && alpha.b <= threshold) {
		alpha.a = 0.0;
	}
	
	COLOR = alpha;
}"

